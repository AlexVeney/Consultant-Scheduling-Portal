         <font>
            <Font name="Apple SD GothicNeo ExtraBold" size="13.0" />
         </font></Label>
      <Label fx:id="appLabel" layoutX="51.0" layoutY="24.0" prefHeight="43.0" prefWidth="307.0" text="Scheduling System" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Apple SD Gothic Neo SemiBold" size="30.0" />
         </font>
      </Label>
    </children>
   <effect>
      <InnerShadow />
   </effect>
</AnchorPane>
