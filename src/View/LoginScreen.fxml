<AnchorPane id="AnchorPane" prefHeight="266.0" prefWidth="409.0" style="-fx-background-color: #143F59;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginScreenController">
    <children>
        <Button fx:id="submitButton" layoutX="259.0" layoutY="203.0" onAction="#signInOnAction" style="-fx-background-color: #657D8C;" text="Submit" textFill="WHITE">
