            <Font name="Apple SD GothicNeo ExtraBold" size="13.0" />
         </font></Button>
      <TextField fx:id="usernameTextField" layoutX="146.0" layoutY="93.0" style="-fx-background-color: #657D8C;">
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></TextField>
      <PasswordField fx:id="passwordTextField" layoutX="146.0" layoutY="137.0" style="-fx-background-color: #657D8C;">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <Lighting>
               <bumpInput>
                  <Shadow />
               </bumpInput>
               <light>
                  <Light.Distant />
               </light>
            </Lighting>
         </effect></PasswordField>
      <Label fx:id="usernameLabel" layoutX="55.0" layoutY="98.0" text="Username:" textFill="WHITE">
